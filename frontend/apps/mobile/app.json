{
    "expo": {
        "name": "Abrechnung",
        "slug": "abrechnung",
        "version": "0.2.0",
        "orientation": "portrait",
        "icon": "./assets/logo.png",
        "scheme": "abrechnung",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/logo_splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "lol.sft.abrechnung"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/logo.png",
                "backgroundColor": "#ffffff"
            },
            "package": "lol.sft.abrechnung"
        },
        "web": {
            "favicon": "./assets/logo.png"
        }
    }
}
